// 0 = allow, 1 = warn, 2 = error
{
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "es2020": true
  },
  "parserOptions": {
    "ecmaVersion": 11,
    "sourceType": "module"
  },
  "rules": {
    "quotes": [1, "single"],
    "no-debugger": [2],
    "complexity": [2],
    "semi": [2, "never"]
  }
}